#~/.local/bin/hdmi-check &
xset s off -dpms &
xset b 0 0 0 &
#~/.fehbg &
feh --bg-fill ~/.wallpapers/nature_oR4qeXc.jpg
fcitx &
nm-applet &
redshift &
#~/.local/bin/blurme
dunst -config ~/.config/dunst/dunstrc &
sleep 1 && compton -b --config ~/.config/compton.blur.conf &
sleep 1 && polybar top &

